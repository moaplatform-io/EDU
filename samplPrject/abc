	<script type="text/javascript">
	    function setCookie( name, value, expirehours ) { 
	        var todayDate = new Date();
	        todayDate = new Date(parseInt(todayDate.getTime() / 86400000) * 86400000 + 54000000);
	        if(todayDate > new Date()) {
	        	expirehours = expirehours - 1;
	        }
	        todayDate.setDate( todayDate.getDate() + expirehours ); 
	        document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";" 
	    } 
	    function closeWin() { 
	        document.getElementById('layer_popup').style.display = "none";
	    }
	    function todaycloseWin() { 
	        setCookie( "ncookie", "done" , 1); 
	        document.getElementById('layer_popup').style.display = "none";
	    }
	</script>
	<style type="text/css">
	.layer_popup {position:fixed;left:0;width:100%;top:0;bottom:0;background:rgba(0,0,0,0.7);z-index:9990;}
	.layer_contents {position:absolute;left:20px;right:20px;top:50px;max-width:400px;margin:auto;z-index:9999;}
	.layer_contents img {width:100%;}
	.layer_contents .layer_btm {margin-top:-10px;padding:3px 5px;color:#fff;text-align:left;background:#000;overflow:hidden;}
	.layer_contents .layer_btm a {float:right;}
	.layer_contents .layer_btm input, .layer_popup .layer_btm label {vertical-align:middle;}
	</style>
