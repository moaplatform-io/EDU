<script>
var pageCode = "00-00-00";
var pageName = "메인페이지";
$(function(){
	
	$(".bx_add_msg .close").click(function(e){
		e.preventDefault();
		$(this).parent().hide();
		var tomorrow = new Date();
		tomorrow.setDate(tomorrow.getDate() + 1);
		document.cookie = "appstorelayer=1; path=/; expires=" + tomorrow;
	});
	
	
	$(".main_info_lottery").tabs({active: "index"});
	$(".main_info_lottery").find(">ul>li>a").click(function(e){e.preventDefault();});
	$(".tab_num_info li").find("a").click(function(e){
		e.preventDefault();
		$(".tab_num_info li").removeClass("on");
		$(this).parent().addClass("on");
		$(".slick_slide").slick("refresh");
	});
	
	$(".lt_group").slick({
		slidesToShow: 1,
        infinite: false,
        arrows: true,
        dots: true
	});
	$(".yg_group").slick({
		slidesToShow: 1,
        infinite: false,
        arrows: true,
        dots: true
	});
	$(".spt_group").slick({
		slidesToShow: 1,
        infinite: false,
        arrows: true, // 180821
        dots: true, // 180821
        // swipe: false
	});
	
	$(".lt_group, .yg_group, .spt_group").on("beforeChange", function(event, slick, currentSlide, nextSlide){
		// nothing
	});
});

$(document).ready(function(){
	//$(".main_info_lottery").tabs({active: "1"});
	//e.preventDefault();
	//$("#ui-id-1").css("background", '#007bc3');
	$(".tab_num_info li").find("a").click($(".main_info_lottery").tabs({active: "1"}));
	$(".tab_num_info li").removeClass("on");
	$(".yg720").addClass("on");
	$(".slick_slide").slick("refresh");
	
	$(function(){
	    $('#tip_list').vTicker({
	        speed: 500,
	        pause: 3000,
	        animation: 'fade',
	        mousePause: false,
	        showItems: 3
	    });
	});
	
	openMainBanner('main_banner');
	
	//프로토콜 검사
	var curPage = document.location.href;
	var mainMobileBannerUrl = "/common.do?method=getMainMobileBannerList";
	
	if(curPage.indexOf("https") > -1){
		mainMobileBannerUrl = "/userSsl.do?method=getMainMobileBannerList";
	}

	$.ajax({
		type: 'POST',
	    url: mainMobileBannerUrl,
	    data : "",
	    dataType    :'json',
	    async:false,
		success : function(data) { // 성공했을때 콜백함수
			var mainMobileBanner = "";
			
			if(data.returnValue == "fail"){
				//alert("사용중인 배너가 없습니다.");
				return;
			}
			
			if (data.mainMobileBannerList == null || data.mainMobileBannerList == "") {
				mainMobileBanner = "";
			} else {
				mainMobileBanner += "<div id='main_banner' class='main_banner'>";

				for(var i=0; i<data.mainMobileBannerList.length ; i++) {
					mainMobileBanner += "<a href='"+data.mainMobileBannerList[i].link+"' target='"+data.mainMobileBannerList[i].target+"' class='main_banner_link'><img src='/img/event/main_banner/"+data.mainMobileBannerList[i].physclFileNm+"'></a>";
					mainMobileBanner += "<input type='checkbox' id='banner1' class='main_banner_close' onclick='javascript:closeMainBanner("+'"main_banner"'+", 1);'/><label for='banner1'>창닫기</label>";
				}
				
				mainMobileBanner += "</div>";
			}

			$("#main_mobile_banner").html(mainMobileBanner);
		},
		error : function(data) {
			//alert("사용중인 배너가 없습니다.");
		}
	});

});
</script>
