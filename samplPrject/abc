	    cookiedata = document.cookie; 
	    if ( cookiedata.indexOf("ncookie=done") < 0 ){ 
	        document.getElementById('layer_popup').style.display = "block";
	    } else {
	        document.getElementById('layer_popup').style.display = "none"; 
	    }
	</script>

